{
  "type": "dark",
  "colors": {
	
    "focusBorder": "#2a1770c7", 
    "dropdown.border": "#22184b00",
    "editorHoverWidget.border": "#956adac2",
    "editorGroup.border": "#1d2520", 
    "editorGroupHeader.tabsBorder": "#3313461c",
    "tab.lastPinnedBorder": "#4792c493", 

    "sideBar.foreground": "#b8aeaee0",
    "badge.foreground": "#b64bb6", 
    "errorForeground": "#922a2a", 
    "dropdown.foreground": "#ffffff",
    "list.highlightForeground": "#c4c9df7a", 
    "pickerGroup.foreground": "#9b8288", 
    "editor.foreground": "#e8d0ff",
    "editorLineNumber.activeForeground": "#b64bb6",
    "editorCursor.foreground": "#b64bb6", 
    "editorWhitespace.foreground": "#3d538f", 
    "ports.iconRunningProcessForeground": "#343533", 

    "debugToolBar.background": "#07141d", 
    "titleBar.activeBackground": "#081a25", 
    "statusBar.background": "#0c2d55", 
    "statusBarItem.remoteBackground": "#1e4775",
    "statusBar.noFolderBackground": "#0ca57fc2", 
    "statusBar.debuggingBackground": "#e22a96", 
    "activityBar.background": "#081a25b0", 
    "progressBar.background": "#365e48f5",
    "badge.background": "#00000050", 
    "sideBar.background": "#071722",
    "editorGroup.dropBackground": "#0f283a",
    "peekViewResult.background": "#2d148aab", 
    "tab.inactiveBackground": "#001c40",
    "editorGroupHeader.tabsBackground": "#10054b50", 
    "editor.lineHighlightBackground": "#8c34df00", 
    "editorWidget.background": "#071d14", 
    "editorHoverWidget.background": "#101b24", 
    "editor.selectionBackground": "#3b1399d2",
    "minimap.selectionHighlight": "#056f8f41",
    "quickInputList.focusBackground": "#413144de", 
    "list.activeSelectionBackground": "#58353560", 
    "list.inactiveSelectionBackground": "#1c381f7e", 
    "list.hoverBackground": "#38272773",
    "editor.background": "#07141d",
    "input.background": "#29354bd7", 
    "dropdown.background": "#261644f5" 

  },
  "tokenColors": [
    { "settings": { "background": "#002451", "foreground": "#FFFFFF" } },

    {
      "name": "Comment", 
      "scope": "comment",
      "settings": { "fontStyle": "italic", "foreground": "#22923e" }
    },
    {
      "name": "Foreground, Operator",
      "scope": "keyword.operator.class, keyword.operator, constant.other, source.php.embedded.line",
      "settings": { "fontStyle": "", "foreground": "#ffffff" }
    },

    {
      "name": "Tag Link, Regular Expression, GitGutter deleted", 
      "scope": " support.other.variable, string.other.link, string.regexp, meta.tag, declaration.tag, markup.deleted.git_gutter",
      "settings": { "fontStyle": "", "foreground": "#70ffbc" } 
    },
    {
      "name": "name tag",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#699fe6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable, Paramaters",
      "scope": "variable, variable.parameter",
      "settings": { "fontStyle": "", "foreground": "#54a6cc" }
    },
    {
      "name": "Number, ascii characters",
      "scope": "constant.numeric, constant.language, support.constant, constant.character, punctuation.section.embedded, keyword.other.unit",
      "settings": { "foreground": "#d859d8c2"}
    },
    {
      "name": "Class, Support",
      "scope": "entity.name.class, entity.name.type, entity.name.namespace, entity.name.scope-resolution, support.class",
      "settings": { "fontStyle": "", "foreground": "#46b56b" } 
    },
    {
      "name": "json fix",
      "scope": "support.type",
      "settings": {
        "foreground": "#73b0ff",
        "fontStyle": ""
      }
    },
    {
      "name": "Symbols, Inherited Class, Markup Heading, GitGutter inserted",
      "scope": "constant.other.symbol, entity.other.inherited-class, markup.heading, markup.inserted.git_gutter",
      "settings": { "fontStyle": "", "foreground": "#f7a0a0" }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#ff9797", 
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": "keyword.operator, constant.other.color",
      "settings": { "foreground": "#aaaaaa" } 
    },
    {
      "name": "New keyword and return",
      "scope": "keyword.operator.new, keyword.control.return",
      "settings": { "foreground": "#ff06eaf6" } 
    },

    {
      "name": "Function, Special Method, Block Level, GitGutter changed",
      "scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.block-level, markup.changed.git_gutter",
      "settings": { "fontStyle": "", "foreground": "#ffa135" } 
    },
    {
      "name": "Keyword, Storage",
      "scope": "keyword, storage, storage.type, entity.name.tag.css",
      "settings": { "fontStyle": "", "foreground": "#cb83ff" }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": { "fontStyle": "", "foreground": "#ec1e1e" }
    },
    

    { "scope": "token.info-token", "settings": { "foreground": "#6796e6" } },
    { "scope": "token.warn-token", "settings": { "foreground": "#cd9731" } },
    { "scope": "token.error-token", "settings": { "foreground": "#f44747" } },
    { "scope": "token.debug-token", "settings": { "foreground": "#b267e6" } }
  ],
  "semanticHighlighting": true
}


// -nl (:
